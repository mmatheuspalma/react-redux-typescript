{
   "name": "foodcourt",
   "version": "0.1.0",
   "private": true,
   "dependencies": {
      "@material-ui/core": "^3.9.1",
      "@material-ui/icons": "^3.0.2",
      "@types/material-ui": "^0.21.5",
      "@types/react-redux": "^7.0.1",
      "@types/redux-logger": "^3.0.6",
      "axios": "0.18.0",
      "node-sass-chokidar": "1.3.4",
      "npm-run-all": "4.1.5",
      "raven-js": "^3.27.0",
      "react": "16.6.3",
      "react-dom": "16.6.3",
      "react-intl": "^2.8.0",
      "react-redux": "^6.0.0",
      "react-router-dom": "4.3.1",
      "react-scripts-ts": "3.1.0",
      "redux": "^3.7.2",
      "redux-devtools-extension": "^2.13.2",
      "redux-logger": "^3.0.6",
      "redux-thunk": "^2.2.0",
      "yarn": "1.12.3"
   },
   "pre-commit": [
      "precommit"
   ],
   "scripts": {
      "precommit": "yarn lint",
      "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src",
      "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
      "start-ts": "react-scripts-ts start",
      "start": "npm-run-all -p watch-css start-ts",
      "build-ts": "react-scripts-ts build",
      "build": "npm-run-all build-css build-ts",
      "test": "react-scripts-ts test --env=jsdom --verbose",
      "eject": "react-scripts-ts eject",
      "lint": "concurrently \"yarn tslint -p .\" \"yarn tsc --noEmit\""
   },
   "devDependencies": {
      "@types/jest": "23.3.10",
      "@types/node": "9.6.40",
      "@types/react": "16.7.8",
      "@types/react-dom": "16.0.11",
      "@types/react-intl": "2.3.14",
      "@types/react-router-dom": "4.3.1",
      "concurrently": "4.0.1",
      "eslint": "5.5.0",
      "pre-commit": "1.2.2",
      "tslint": "5.11.0",
      "tslint-eslint-rules": "5.4.0",
      "tslint-react": "3.6.0",
      "typescript": "3.2.1"
   }
}
